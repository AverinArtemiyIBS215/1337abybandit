№11
a = int(input("Введите точку начала отрезка: "))
b = int(input("Введите точку конца отрезка: "))
while a <= b:
    print(2.71 ** a - 1)
    a += 1
№12
a = int(input("Введите точку начала отрезка: "))
b = int(input("Введите точку конца отрезка: "))
while a <= b:
    print(1 / 2 * a)
    a += 1
№13
a = int(input("Введите точку начала отрезка: "))
b = int(input("Введите точку конца отрезка: "))
while a <= b:
    print((a - 1) ** 2)
    a += 1 
№14
a = 1 
b = 20 
while a <= b:
    t = a + 1 
    print(2 * t ** 2 + 2 * t + 2)
    a += 1
№15
a = 2
b = 20 
while a <= b:
    f = 2 * a 
    print(8 * f ** 3 - f)
    a += 2
№6
a = int(input())
while a != 0:
    a -= 1
    print(a)
№7
a = int(input())
b = 0
c = 1
while b <= a:
    b += 1 / c
    c += 1
    print(f"{c} ({b})") 
#___№8___
a = int(input())
b = 1
while b ** 2 < a:
    b += 1
print(f"{b} ({b ** 2})")
